        xmlns:local="clr-namespace:CadastroTelefone.View"
        mc:Ignorable="d"
        Title="CadastroTelefone" Height="164.109" Width="443.288">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="128*"/>
            <ColumnDefinition Width="323*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="tbxNumero" Margin="10,39,10,54" TextWrapping="Wrap" Grid.Column="1"/>
